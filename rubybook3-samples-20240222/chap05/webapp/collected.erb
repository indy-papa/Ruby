<%# encoding: UTF-8 %>
<% require_relative './term_names' %>
<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <link rel="stylesheet" type="text/css" href="default.css">
    <title>Amazon注文履歴</title>
  </head>
  <body>
    <h1>Amazon注文履歴</h1>
    <h2>注文履歴が取得できました</h2>
    <p id="links"><a href="list02.html">取得済み注文履歴一覧の表示</a>｜
      <a href="index.html">メニュー画面に戻る</a></p>
    <p id="msg">下記の注文履歴を取得しました</p>
    <hr>
    <table>
      <tr>
        <th>取得期間</th>
        <td><%= $term_names[term] %>（<%= term %>）</td> <!--1-->
      </tr>
      <tr>
        <th>保存ファイル名</th>
        <td><%= file %></td> <!--2-->
      </tr>
    </table>
    <hr>
  </body>
</html>
